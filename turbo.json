{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": [".build/**"],
			"dependsOn": ["^build"],
			"cache": true
		},
		"dev": {
			"outputs": [".build/**"],
			"dependsOn": ["^build"],
			"cache": true
		},
		"dev.debug": {
			"outputs": [".build/**"],
			"dependsOn": ["^build"],
			"cache": true
		}
	}
}
